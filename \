from matplotlib import pyplot as plt
import math

print("program 1")
amplitude = 169.7
loops = 2
for a in range(0, 360 * loops, 10):
    v = amplitude * math.sin(a * math.pi / 180)
    plt.scatter(a, v, s=5, c="r")  # plot x,y point with size and color
plt.show()

print("program 2")
amplitude = 154.3
loops = 2
for a in range(0, 360 * loops, 10):
    i = amplitude * math.cos(a * math.pi / 180)
    plt.scatter(a, i, s=5, c="r")  # plot x,y point with size and color
plt.show()
